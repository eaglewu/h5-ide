<!-- {{stack}} -->
<div class="option-group-head expand">
    Health Monitor({{list.length}})
</div>
<div class="option-group">
    <section class="group item-readable-list">
    {{#each list}}
    <div class="item hm-item" data-id="{{id}}">
        <div class="item-name">{{name}}</div>
        <div class="item-info">{{type}} | {{url_path}}</div>
    </div>
    {{/each}}
    </section>
</div>
