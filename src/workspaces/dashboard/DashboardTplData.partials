<!-- {{globalMap}} -->
{{#each this}}
<li id="{{region}}" class="global-map-item" title="{{regionName}} Dashboard">
  <h5>{{regionName}}</h5>
  <span class="global-region-location app">{{app}}</span>
  <span class="global-region-location stack">{{stack}}</span>
</li>
{{/each}}

<!-- {{recent}} -->
<section class="hide">
{{#if apps.length}}
  <h4>{{i18n "DASH_LBL_RECENT_LAUNCHED_STACK"}}</h4>
  <ul>{{#each apps}}<li class="recent-list-item" data-id="{{id}}">
    <h5>{{name}} {{#if usage}}<i class="right icon-app-type-{{usage}}"></i>{{/if}}</h5>
    <span>{{regionName}}</span>
    <span class="recent-time">{{formatedTime}}</span>
  </li>{{/each}}</ul>
{{else}}
  <span class="empty-text">No recently launched app in 30 days</span>
{{/if}}
</section>
<section>
{{#if stacks.length}}
  <h4>{{i18n "DASH_LBL_RECENT_EDITED_STACK"}}</h4>
  <ul>{{#each stacks}}<li class="recent-list-item" data-id="{{id}}">
    <h5>{{name}}</h5>
    <span>{{regionName}}</span>
    <span class="recent-time">{{formatedTime}}</span>
  </li>{{/each}}</ul>
{{else}}
  <span class="empty-text">No recently edited stack in 30 days</span>
{{/if}}
</section>

<!-- {{globalResources}} -->
<ul id="global-region-resource-list" class="clearfix">
  <li id="global-region-resource-instance">
    <div class="global-region-resource-header">
      <h5>{{i18n "DASH_LBL_RUNNING_INSTANCE"}}</h5>
      <div class="global-region-resource-count-wrap">
        <i id="global-resource-icon-instance" class="global-resource-icon"></i>
        <span class="global-region-resource-count">{{instances.total}}</span>
      </div>
    </div>

    <div class="global-region-resource-content">
      <ul>
        {{#each instances.data}}
          <li data-region="region">
            <h4>{{city}}</h4>
            <h5>{{area}}</h5>
            <span class="global-region-resource-location-count">{{count}}</span>
          </li>
        {{/each}}
      </ul>
    </div>
  </li>

  <li id="global-region-resource-eip">
    <div class="global-region-resource-header">
      <h5>{{i18n "DASH_LBL_ELASTIC_IP"}}</h5>
      <div class="global-region-resource-count-wrap">
        <i id="global-resource-icon-eip" class="global-resource-icon"></i>
        <span class="global-region-resource-count">{{eip.total}}</span>
      </div>
    </div>

    <div class="global-region-resource-content">
      <ul>
        {{#each eip.data}}
          <li>
            <h4>{{city}}</h4>
            <h5>{{area}}</h5>
            <span class="global-region-resource-location-count">{{count}}</span>
          </li>
        {{/each}}
      </ul>
    </div>
  </li>

  <li id="global-region-resource-volume">
    <div class="global-region-resource-header">
      <h5>{{i18n "DASH_LBL_VOLUME"}}</h5>
      <div class="global-region-resource-count-wrap">
        <i id="global-resource-icon-vol" class="global-resource-icon"></i>
        <span class="global-region-resource-count">{{volume.total}}</span>
      </div>
    </div>

    <div class="global-region-resource-content">
      <ul>
        {{#each volume.data}}
          <li>
            <h4>{{city}}</h4>
            <h5>{{area}}</h5>
            <span class="global-region-resource-location-count">{{count}}</span>
          </li>
        {{/each}}
      </ul>
    </div>
  </li>

  <li id="global-region-resource-elb">
    <div class="global-region-resource-header">
      <h5>{{i18n "DASH_LBL_LOAD_BALANCER"}}</h5>
      <div class="global-region-resource-count-wrap">
        <i id="global-resource-icon-elb" class="global-resource-icon"></i>
        <span class="global-region-resource-count">{{elb.total}}</span>
      </div>
    </div>

    <div class="global-region-resource-content">
      <ul>
        {{#each elb.data}}
          <li>
            <h4>{{city}}</h4>
            <h5>{{area}}</h5>
            <span class="global-region-resource-location-count">{{count}}</span>
          </li>
        {{/each}}
      </ul>
    </div>
  </li>

  <li id="global-region-resource-vpn">
    <div class="global-region-resource-header">
      <h5>{{i18n "DASH_LBL_VPN"}}</h5>
      <div class="global-region-resource-count-wrap">
          <i id="global-resource-icon-vpn" class="global-resource-icon"></i>
          <span class="global-region-resource-count">{{vpn.total}}</span>
      </div>
    </div>

    <div class="global-region-resource-content">
      <ul>
        {{#each vpn.data}}
