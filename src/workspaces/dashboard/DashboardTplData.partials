<!-- {{region_app_stack}} -->
<ul id="region-resource-tab" class="clearfix">
    <li {{#if app}}class="on"{{/if}}>{{apps.length}} <small>{{i18n "DASH_LBL_APP"}}</small></li>
    <li class="{{#if stack}}on {{/if}}stack">{{stacks.length}} <small>{{i18n "DASH_LBL_STACK"}}</small>
        <button id="btn-create-stack" class="icon-new-stack" data-region="{{region}}" title="{{i18n 'TOOL_TIP_CREATE_STACK'}}"></button>
    </li>
</ul> <!-- Resource tab -->

<div class="scroll-wrap">
<div class="scrollbar-veritical-wrap" style="display: block;"><div class="scrollbar-veritical-thumb"></div></div>
<div class="scroll-content">

<ul id="region-resource-app-wrap" class="region-resource-list clearfix" {{#if stack}} style="display: none;"{{/if}}>

{{#each apps}}
{{#if progressing}}
<li class="disabled" data-appid="{{id}}">
    <div class="thumbnail app-thumbnail"></div>
    <div class="region-resource-progess{{#unless progress}} hide{{/unless}}" style="width:{{progress}}%;"></div>
    <div class="region-resource-info truncate">
        <div class="loading-spinner loading-spinner-small"></div>{{name}} - {{stateDesc}}...
    </div>
</li>
{{else}}
<li data-id="{{id}}">
    {{#if usage}}<i class="icon-app-type-{{usage}}"></i>{{/if}}
    <div class="thumbnail app-thumbnail"> <img src="{{thumbnail}}" {{#unless thumbnail}}class="hide"{{/unless}}/> </div>
    <div class="region-resource-info">
        <i class="icon-terminate terminate-app"></i>
        {{#if stoppable}}
            {{#ifCond stateDesc 'Running'}}
                <i class="icon-stop stop-app"></i>
            {{else}}
                <i class="icon-play start-app"></i>
            {{/ifCond}}
        {{/if}}
        <span class="{{#ifCond stateDesc 'Running'}}running{{else}}stopped{{/ifCond}} truncate">{{name}}</span>
    </div>
</li>
{{/if}}
{{else}}
    <div class="blank-widget"><div>{{i18n "DASH_LBL_NO_APP"}}</div></div>
{{/each}}
</ul> <!-- Resource App -->

<ul id="region-resource-stack-wrap" class="region-resource-list clearfix" {{#if app}} style="display: none;"{{/if}}>
{{#each stacks}}
    <li data-id="{{id}}">
        <div class="thumbnail"> <img src="{{thumbnail}}" {{#unless thumbnail}}class="hide"{{/unless}}/> </div>
        <div class="region-resource-info">
            <i class="icon-delete delete-stack"></i>
            <i class="icon-duplicate duplicate-stack"></i>
            <span class="truncate">{{name}}</span>
        </div>
    </li>
{{else}}
    <div class="blank-widget"><div>{{i18n "DASH_LBL_NO_STACK"}}</div></div>
{{/each}}
</ul> <!-- Resource Stack -->
</div>
</div>
<!-- Region App & Stack -->


<!-- {{region_resource_head}} -->
<!-- Region AWS resource -->
<div id="region-aws-resource-wrap">
    {{#with cur_region_resource_info}}
        <div id="region-aws-resource-tab">
            <ul class="clearfix">
                <li class="region-resource-tab-item on" data-resource-type="DescribeInstances">
                    <i id="global-resource-icon-instance" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{Instance}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_INSTANCE"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeAddresses">
                    <i id="global-resource-icon-eip" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{EIP}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_ELASTIC_IP"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeVolumes">
                    <i id="global-resource-icon-vol" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{Volume}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_VOLUME"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeVpcs">
                    <i id="global-resource-icon-vpc" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{VPC}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_VPC"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeVpnConnections">
                    <i id="global-resource-icon-vpn" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{VPN}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_VPN"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeLoadBalancers">
                    <i id="global-resource-icon-elb" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{ELB}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_LOAD_BALANCER"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeAutoScalingGroups">
                    <i id="global-resource-icon-asg" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{AutoScalingGroup}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_AUTO_SCALING_GROUP"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="DescribeAlarms">
                    <i id="global-resource-icon-cwa" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{CW}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_CLOUDWATCH_ALARM"}}</span>
                </li>

                <li class="region-resource-tab-item" data-resource-type="ListSubscriptions">
                    <i id="global-resource-icon-sns" class="global-resource-icon"></i>
                    <span class="region-resource-count">{{SNS}}</span>
                    <span class="region-resource-name">{{i18n "DASH_LBL_SNS_SUBSCRIPTION"}}</span>
                </li>
            </ul>
        </div>
    {{/with}}

    <div id="region-aws-resource-data">

    </div>

</div>

<!-- {{DescribeInstances}} -->
<div class="table-head-fix" id="DescribeInstances">
    <table class="table-head">
        <thead>
            <tr>
                <th class="sortable" style="width: 25%">{{i18n "DASH_LBL_INSTANCE_NAME"}}/{{i18n "DASH_LBL_ID"}}</th>
                <th class="sortable" style="width: 17%">{{i18n "DASH_LBL_STATUS"}}</th>
                <th class="sortable" style="width: 20%" data-row-type="datetime">{{i18n "DASH_LBL_LAUNCH_TIME"}}</th>
                <th class="sortable" style="width: 15%">{{i18n "DASH_LBL_AMI"}}</th>
                <th class="sortable" style="width: 15%">{{i18n "DASH_LBL_AVAILABILITY_ZONE"}}</th>
                <th class="sortable" style="width: 8%">{{i18n "DASH_LBL_DETAIL"}}</th>
            </tr>
        </thead>
    </table>
    <div class="scroll-wrap">
        <div class="scrollbar-veritical-wrap"><div class="scrollbar-veritical-thumb"></div></div>
        <div class="scroll-content">
            <table class="table">
                <thead>
                    <tr>
                        <th style="width: 25%"><span class="th-inner"></span></th>
                        <th style="width: 17%"><span class="th-inner"></span></th>
                        <th style="width: 20%"><span class="th-inner"></span></th>
                        <th style="width: 15%"><span class="th-inner"></span></th>
                        <th style="width: 15%"><span class="th-inner"></span></th>
                        <th style="width: 8%"><span class="th-inner"></span></th>
                    </tr>
                </thead>
                <tbody>
                    {{#each DescribeInstances }}
                    <tr>
                        <td>
                            <div>
                                {{#if app}}
                                <span class="table-app-link truncate tooltip {{#if clickAble}}table-app-link-clickable{{/if}}" data-tooltip="{{i18n 'DASH_TIP_APP_CREATED_BY'}}{{ owner }}" data-id="{{tagSet.app-id}}" data-name="{{tagSet.app}}">[{{ app }}]</span>
                                {{else}}
                                <span class="table-app-link truncate table-app-link-unmanaged tooltip" data-tooltip='{{ i18n "DASH_TIP_UNMANAGED_RESOURCE" }}' >{{ i18n "DASH_LBL_UNMANAGED" }}</span>
                                {{/if}}
                            </div>
                            <div>
                                {{ host }}
                                <span class="resource-id">( {{ instanceId }} )</span>
                            </div>
                        </td>
                        <td><i class="status status-{{instanceState.name}} icon-label"></i>{{ instanceState.name }}</td>
                        <td>{{ launchTime }}</td>
                        <td><a href="javascript:void(0)" class="bubble table-link"  data-bubble-template="bubbleAMIInfo" data-bubble-data='{"id":"{{imageId}}","name":"{{ image.name }}","description":"{{image.description}}","architecture":"{{image.architecture}}","imageLocation":"{{image.imageLocation}}","isPublic":"{{image.isPublic}}","kernelId":"{{image.kernelId}}","rootDeviceName":"{{image.rootDeviceName}}","rootDeviceType":"{{image.rootDeviceType}}"}'>{{ imageId }}</a></td>
                        <td>{{ placement.availabilityZone}}</td>
                        <td class="table-action"><a href="javascript:void(0)" title="{{i18n 'DASH_TIT_VIEW_RESOURCE_DETAIL'}}" class="icon-detail modal" data-modal-template="modalRegionResourceDetail" data-modal-data='{{{detail}}}' data-modal-dismiss="true"><i class="icon-list-2"></i></a></td>
                    </tr>
                    {{else}}
                    <div class="dashboard-widget-body dashboard-widget-body-table table-tall blank-widget">
                        <p>{{i18n "DASH_TIP_NO_RESOURCE_LEFT"}}{{i18n "DASH_LBL_INSTANCE"}}{{i18n "DASH_TIP_NO_RESOURCE_RIGHT"}}.</p>
                    </div>
                    {{/each}}

                </tbody>
            </table>
        </div>
    </div>
</div>

