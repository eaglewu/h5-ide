<!-- {{frame}} -->
<div class="fake-input">
    <ul class="tags"></ul>
    <input type="text"/>
    <span class="line-tip"></span>
</div>
<ul class="dropdown"></ul>

<!-- {{dropdown}} -->
{{#each this}}
<li data-type="{{type}}" data-value="{{value}}" class="{{#ifCond type 'label'}}label{{else}}option{{/ifCond}} {{#if selected}}selected{{/if}}">
{{{text}}}
</li>
{{/each}}

<!-- {{tag}} -->
{{#each this}}<li data-key={{key}} data-value={{value}}>{{key}}{{#if value}}={{value}}{{/if}}</li>{{/each}}