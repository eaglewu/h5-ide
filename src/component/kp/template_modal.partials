<!-- {{frame}} -->
<div id="modal-kp-manage" class="modal-toolbar">
    <div class="modal-header">
        <h3>Manage Key Pairs in {{regionName}}</h3>
        <i class="modal-close">Ã—</i>
    </div>

    <div class="modal-body">
        <div class="content-wrap">
            <div class="toolbar">
                <button class="icon-new-stack btn btn-blue" id="create-kp">Create Key Pair</button>
                <div class="btn-group">
                    <button class="icon-import tooltip" id="kp-import" data-tooltip=""></button>
                    <button class="icon-del tooltip" id="kp-delete" data-tooltip=""></button>
                    <button class="icon-refresh tooltip" id="kp-refresh" data-tooltip=""></button>
                </div>
            </div>
            <div class="list">
                <div class="slidebox">
                    <div class="content clearfix">
                    </div>
                    <div class="error">
                        something wrong
                    </div>
                </div>
                <div class="table-head-fix">
                        <table class="table-head">
                            <thead>
                                <tr>
                                    <th>
                                        <div class="checkbox">
                                            <input id="kp-select-all" type="checkbox" value="None">
                                            <label for="kp-select-all"></label>
                                        </div>
                                    </th>
                                    <th class="sortable">Name</th>
                                    <th>Fingerprint</th>
                                </tr>
                            </thead>
                        </table>
                        <div class="scroll-wrap">
                            <div class="scrollbar-veritical-wrap" style="display: block;"><div class="scrollbar-veritical-thumb"></div></div>
                            <div class="scroll-content" style="display:block;">
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th><div class="th-inner"></div></th>
                                            <th><div class="th-inner"></div></th>
                                            <th><div class="th-inner"></div></th>
                                        </tr>
                                    </thead>
                                    <tbody id="community_ami_table">
                                        {{#each keys}}
                                        <tr class="item" data-id="">
                                            <td>
                                                <div class="checkbox">
                                                    <input id="kp-select-{{keyFingerprint}}" type="checkbox" value="None" data-name="{{keyName}}" class="one-cb">
                                                    <label for="kp-select-{{keyFingerprint}}"></label>
                                                </div>
                                            </td>
                                            <td>{{keyName}}</td>
                                            <td>{{keyFingerprint}}</td>
                                        </tr>
                                        {{/each}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>

            </div>
        </div>
    </div>
</div>

<!-- {{keys}} -->
{{#each keys}}
<tr class="item" data-id="">
    <td class="ami-table-fav">
        <div class="checkbox">
            <input id="kp-select-{{keyFingerprint}}" type="checkbox" value="None" data-name="{{keyName}}" class="one-cb">
            <label for="kp-select-{{keyFingerprint}}"></label>
        </div>
    </td>
    <td class="ami-table-id">{{keyName}}</td>
    <td class="ami-table-size">{{keyFingerprint}}</td>
</tr>
{{/each}}

<!-- {{loading}} -->
<div class="loading-spinner"></div>

<!-- {{slideCreate}} -->
<div class="slide-create" data-bind="true">
    <label for="create-kp-name">Key Pair Name</label>
    <input class="input" type="text" id="create-kp-name" data-ignore="true" data-ignore-regexp="^[a-zA-Z0-9,_-]*$" data-required="true" maxlength="255" placeholder="allow alphanumber, _ or - up to 255 characters">
    <div class="init action">
        <button class="btn btn-blue do-action" data-action="create">Create</button>
        <button class="btn btn-silver cancel">Cancel</button>
    </div>
    <div class="processing action" style="display:none;">
        <button class="btn" disabled>Creating...</button>
    </div>
    <div class="download action" style="display:none;">
        <a class="btn btn-blue do-action pulse" data-action="download" id="download-kp">Download</a>
        <button class="btn btn-silver cancel" disabled>Cancel</button>
    </div>
</div>

<!-- {{slideDelete}} -->
<div class="slide-delete">
    <div class="modal-text-major">Confirm to delete {{#if selecteKeyName }}{{selecteKeyName}}{{else}}selected {{selectedCount}} key pairs{{/if}}?</div>
    <div class="init action">
        <button class="btn btn-red do-action" data-action="delete">Delete</button>
        <button class="btn btn-silver cancel">Cancel</button>
    </div>
    <div class="processing action" style="display:none;">
        <button class="btn" disabled>Deleting...</button>
    </div>
</div>
<!-- {{slideImport}} -->
<div class="slide-import" data-bind="true">
    <label for="import-kp-name">Key Pair Name</label>
    <input class="input" type="text" id="import-kp-name" data-ignore="true" data-ignore-regexp="^[a-zA-Z0-9,_-]*$" data-required="true" maxlength="255" placeholder="allow alphanumber, _ or - up to 255 characters">
    <div class="import-zone">

    </div>
    <div class="init action">
        <button class="btn" disabled>Import</button>
        <button class="btn btn-silver cancel">Cancel</button>
    </div>
    <div class="ready action" style="display:none;">
        <button class="btn btn-blue do-action" data-action="import">Import</button>
        <button class="btn btn-silver cancel">Cancel</button>
    </div>
    <div class="processing action" style="display:none;">
        <button class="btn" disabled>Importing...</button>
    </div>
</div>

<!-- {{upload}} -->
<div id="modal-import-json-dropzone" class="upload-kp-component">
    <p class="upload-stuff">
        Drop {{type}},
        <label for="modal-import-json-file" class="select-file-link">select a file</label>
        <span class="display-empty">or paste the key content here.</span>
        <span class="display-filled" style="display:none;">or paste the key content again to update.</span>
        <input type="file" id="modal-import-json-file">
    </p>
    <p class="key-content"></p>
</div>



