<!-- {{headerNotifyItem}} -->
<li class="notification-item{{#unless readed}} unread{{/unless}}">
  <div class="notification-message">
    {{#if state.failed}}
      <i class="icon-error"></i>
      <div class="content"><span class="resource-name-label">{{targetName}}</span> failed to {{operation}} in {{region}}.</div>
    {{/if}}
    {{#if state.pending}}
      <i class="icon-pending"></i>
      <div class="content">Sending request to {{operation}} <span class="resource-name-label">{{targetName}}</span> in {{region}}.</div>
    {{/if}}
    {{#if state.processing}}
      <i class="icon-pending"></i>
      <div class="content">Processing request to {{operation}} <span class="resource-name-label">{{targetName}}</span> in {{region}}.</div>
    {{/if}}
    {{#if state.completed}}
      <i class="icon-success"></i>
      <div class="content"><span class="resource-name-label">{{targetName}} </span>{{operation}} successfully in {{region}}.</div>
    {{/if}}
  </div>

  {{#if error}}<div class="notification-details">{{error}}</div>{{/if}}

  <div class="notification-duration left">{{duration}}</div>
  <div class="timestamp">{{time}}</div>
</li>

<!-- {{accessTokenTable}} -->
{{#each this}}
<li>
	<input class="tokenName input" value="{{name}}" readonly/>
	<span class="tokenToken click-select truncate tooltip" data-tooltip="{{ i18n 'PROP.ELB_TIP_CLICK_TO_SELECT_ALL'}}">{{token}}</span>
	<span class="tokenControl">
		<button class="tooltip icon-edit" data-tooltip=""></button>
		<button class="tooltip icon-delete" data-tooltip=""></button>
		<button class="btn btn-blue tokenDone">{{ i18n 'HEAD_BTN_DONE' }}</button>
	</span>
</li>
{{/each}}

<!-- {{modalTemplate}} -->
<div class="modal-box{{#ifCond mode "panel"}} panel{{/ifCond}}">
    <div{{#ifCond mode "panel"}}{{else}} class="modal-wrapper-fix"{{/ifCond}}>
        <div class="modal-header">
            <h3>{{ title }}</h3>
            {{#unless hideClose}}<i class="modal-close">×</i>{{/unless}}
        </div>
        <div class="modal-body context-wrap{{#if hasScroll}} scroll-wrap scrollbar-auto-hide{{/if}}"{{#if compact}} style="padding: 0;"{{/if}} >
            {{#if hasScroll}}
            <div class="scrollbar-veritical-wrap"><div class="scrollbar-veritical-thumb"></div></div>
                <div class="scroll-content" {{#if compact}} style="padding: 0;"{{/if}}>
                    {{{ template }}}
                </div>
            {{else}}
            {{{template}}}
            {{/if}}
            </div>
        {{#if hasFooter}}
        <div class="modal-footer">
            {{#unless confirm.hide}}<button class="btn modal-confirm btn-{{confirm.color}}" {{#if confirm.disabled}} disabled {{/if}}>{{confirm.text}}</button>{{/unless}}
            {{#unless cancel.hide}}<button class="btn modal-close btn-silver">{{cancel.text}}</button>{{/unless}}
        </div>
        {{/if}}
    </div>
</div>

