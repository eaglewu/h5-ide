<!-- {{notification.item}} -->
<div class="notification_item {{type}}_item">
	<span>{{template}}</span>
	{{#if should_stay}}<i class="notification_close">&times;</i>{{/if}}
</div>


<!-- {{modalAppToStack}} -->
<div style="width: 520px">
    <div class="modal-header">
        <h3>{{title}}</h3>
        <i class="modal-close">&times;</i>
    </div>
    <div class="modal-body">
        <p class="modal-text-minor">{{intro-1}}</p>
        <p class="modal-text-minor">{{intro-2}}</p>
        <div class="modal-center-align-helper">
            <div class="modal-control-group">
                <div id="replace_stack" style="padding: 10px 0">
                    <div class="radio">
                        <input id="radio-replace-stack" type="radio" name="save-stack-type" checked>
                        <label for="radio-replace-stack"></label>
                    </div>
                    <label class="modal-text-minor" for="radio-replace-stack">{{replace}}</label>
                    <div style="padding: 10px 22px" class="radio-instruction">
                        {{replace-info}} "{{input}}" {{replace-info-end}}
                    </div>
                </div>
                <div id="save_new_stack">
                   <div class="radio">
                       <input id="radio-new-stack" type="radio" name="save-stack-type">
                       <label for="radio-new-stack"></label>
                   </div>
                   <label class="modal-text-minor" for="radio-new-stack">{{save-new}}</label>
                   <div style="padding: 10px 22px" class="radio-instruction hide">
                       <p>{{instruction}}</p>
                       <input class="input" id="modal-input-value" value="{{input}}" type="text" style="width: 400px"/>
                       <div id="stack-name-exist" class="hide" style="color: #ec3c38">{{error-message}}</div>
                   </div>
                </div>
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button id="btn-confirm" class="btn btn-{{color}}">{{confirm}}</button>
        <button class="btn modal-close btn-silver">{{i18n "PROP.LBL_CANCEL"}}</button>
    </div>
</div>


<!-- {{modalApp}} -->
<div style="width:420px">
	<div class="modal-header">
		<h3>{{title}}</h3>
		<i class="modal-close">&times;</i>
	</div>
	<div class="modal-body">
		{{#unless is_production}}
			<div class="modal-text-wraper">
				<div class="modal-center-align-helper">
					{{#if input}}
						<div class="modal-control-group">
							<label class="modal-text-major">{{body}}</label>
							<input id="modal-input-value" class="input" type="text" value="{{input}}" />
						</div>
					{{else}}
						<div class="modal-text-major">
							{{{body}}}
						</div>
						{{#if is_asg}}
							<div class="modal-text-minor">{{ i18n 'POP_CONFIRM_STOP_ASG' }}</div>
						{{/if}}
					{{/if}}
				</div>
			</div>
		{{else}}
			<div class="terminate-pro-app-confirm">
				{{#if is_stop}}
					<p><b>{{app_name}}{{ i18n 'POP_CONFIRM_PROD_APP_WARNING_MSG' }}</b>{{ i18n 'POP_CONFIRM_STOP_PROD_APP_MSG' }}
						{{#if is_asg}}
							{{ i18n 'POP_CONFIRM_STOP_ASG' }}
						{{/if}}
					</p>
					<p>{{ i18n 'POP_CONFIRM_STOP_PROD_APP_INPUT_LBL' }}</p>
				{{else}}
					<p><b>{{app_name}} {{ i18n 'POP_CONFIRM_PROD_APP_WARNING_MSG' }}</b>{{ i18n 'POP_CONFIRM_TERMINATE_PROD_APP_MSG' }}</p>
					<p>{{ i18n 'POP_CONFIRM_TERMINATE_PROD_APP_INPUT_LBL' }}</p>
				{{/if}}
				<p class="confirm-area"><input class="input" id="confirm-app-name" data-confirm="{{app_name}}"/></p>
			</div>
		{{/unless}}
	</div>
	<div class="modal-footer">
		<button id="btn-confirm" class="btn btn-{{color}}" {{#if is_production}}disabled{{/if}}>{{confirm}}</button>
		<button class="btn modal-close btn-silver">{{i18n "TOOLBAR.POP_BTN_CANCEL"}}</button>
	</div>
</div>

<!-- {{modalToolbar}} -->
