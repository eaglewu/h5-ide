<!-- {{modalSGRule}} -->
<div id="modal-sg-rule" data-bind="true">
	<div class="modal-header">
		<h3>{{ i18n "POP_SGRULE_TITLE_ADD" }}</h3><i class="modal-close">&times;</i>
	</div>

	<div class="modal-body">
		<div class="modal-control-group clearfix">
			<label class="label-short">{{ i18n "POP_SGRULE_LBL_DIRECTION" }}</label>

			<div id="sg-modal-direction">
			{{#if isClassic}}
				<div class="modal-text-minor" style="margin-top:10px;"><i class="icon-inbound"></i>{{ i18n "POP_SGRULE_LBL_INBOUND" }}</div>
			{{else}}
			<div class="radio-group-horizontal">
				<div class="radio">
					<input id="radio_inbound" type="radio" name="sg-direction" checked="checked" value="inbound" />
					<label for="radio_inbound"></label>
				</div>
				<label for="radio_inbound" ><i class="icon-inbound icon-label"></i>{{ i18n "POP_SGRULE_LBL_INBOUND" }}</label>
			</div>

			<div class="radio-group-horizontal">
				<div class="radio">
					<input id="radio_outbound" type="radio" name="sg-direction" value="outbound"/>
					<label for="radio_outbound"></label>
				</div>
				<label for="radio_outbound"><i class="icon-outbound icon-label"></i>{{ i18n "POP_SGRULE_LBL_OUTBOUND" }}</label>
			</div>
			{{/if}}
			</div>
		</div>

		<div class="modal-control-group clearfix">
