<!-- {{updateApp}} -->
<div id="app-apply-update">
    <div class="payment-wrapper">
        <div class="modal-control-group default-kp-group clearfix" style="display:none;">
            <label for="kp-runtime-placeholder">$DefaultKeyPair</label>
            <div id="kp-runtime-placeholder"></div>
            <div class="runtime-error" id="runtime-error-kp"></div>
        </div>
        {{#if isRunning}}
            <h3 class="modal-text-major">{{ i18n 'POP_CONFIRM_UPDATE_MAJOR_TEXT_RUNNING'}}</h3>
        {{else}}
            <h3 class="modal-text-major">{{ i18n 'POP_CONFIRM_UPDATE_MAJOR_TEXT_STOPPED'}}</h3>

            <p class="modal-text-minor"
               style="margin-top:10px;">{{ i18n 'POP_CONFIRM_UPDATE_MINOR_TEXT_STOPPED'}}</p>
        {{/if}}
            
        <div class="scroll-wrap res_diff_tree" id="app-update-summary-table" style="max-height:200px;">
            <div class="scrollbar-veritical-wrap"><div class="scrollbar-veritical-thumb"></div></div>
            <div class="scroll-content"></div>
        </div>
        {{#if removeList.length}}
            <section class="check-final-snapshot checkbox-wrap">
                <div class="checkbox">
                    <input id="take-rds-snapshot" type="checkbox" checked="checked" name="dns-resolution">
                    <label for="take-rds-snapshot"></label>
                </div>
                <label for="take-rds-snapshot">Take final snapshot for DB instances.</label>
            </section>
            {{#if notReadyDB.length}}
                <p class="cant-snapshot">DB Instance
                    {{#each notReadyDB}}{{#if @index}}, {{/if}}{{attributes.DBInstanceIdentifier}}(<span
                            class="db-stop-status">{{attributes.DBInstanceStatus}}</span>){{/each}}
                    cannot take final snapshot.</p>
            {{/if}}
        {{/if}}
        <div class="stack-validation">
            <details open style="display:none;">
                <summary>{{ i18n 'POP_CONFIRM_UPDATE_VALIDATION'}}</summary>
                <div id="stack-run-validation-container"></div>
            </details>
            <div class="nutshell">:<label></label></div>
            <div class="validating">
                <div class="loading-spinner loading-spinner-small"></div>
                <p>{{ i18n 'POP_CONFIRM_UPDATE_VALIDATING'}}</p>
            </div>
        </div>
    </div>
    <div class="payment-wrapper-right">
        <div class="estimate clearfix">
            <div class="title">Estimated AWS Cost</div>
            <div class="price" id="label-total-fee"><b>${{total_fee}}</b> / month</div>
        </div>
        <div class="estimate-visualops clearfix hide">
            <div class="title">Estimated VisualOps Cost</div>
            <div class="price" id="label-visualops-fee"><b>${{visualops_fee}}</b> / month</div>
        </div>
        {{#ifCond paymentState "past_due"}}
            <div class="payment-warning">
                There was an issue to process payment in your account. Please update your <a href="{{payment-link}}">payment
                information</a>.
            </div>
        {{/ifCond}}
    </div>

</div>


